# Copy this file to `.env` and fill in the values.
# Never commit real secrets (private keys, API keys) to Git.

# Deployer EOA private key (without 0x prefix).
PRIVATE_KEY=

# RPC endpoints (used by foundry.toml [rpc_endpoints])
SEPOLIA_RPC_URL=
JOVAY_TESTNET_RPC_URL=

# Optional (only needed if you use `--verify` on supported explorers)
ETHERSCAN_API_KEY=

# Optional script parameters (set when running scripts)
SENDER_ADDRESS=
RECEIVER_ADDRESS=
MESSAGE=

TOKEN_TRANSFEROR=
RECEIVER=
AMOUNT=
TOKEN=

# Optional overrides (defaults are hardcoded in scripts)
ROUTER=
LINK=
